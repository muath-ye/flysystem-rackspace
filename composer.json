{
    "name": "muath-ye/flysystem-rackspace",
    "description": "Flysystem adapter for Rackspace",
    "license": "MIT",
    "authors": [
        {
            "name": "Frank de Jonge",
            "email": "info@frenky.net"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "league/flysystem": "^1.0|^2.0|^3.0",
        "rackspace/php-opencloud": "^1"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7|^6.0|^7.0|^8.0|^9.0",
        "mockery/mockery": "~0.9|^1.0"
    },
    "autoload": {
        "psr-4": {
            "League\\Flysystem\\Rackspace\\": "src/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
